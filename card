#lang racket

(provide cartaVacia)
(provide cartaInicial)
(provide cartaVacia?)
(provide addSymbol)

; TDA CARTA
; Representacion
; Carta = CartaVacia | lista de N simbolos

; Constructor
(define cartaVacia null)
(define cartaInicial list)
; Pertenencia
(define cartaVacia? null?)

; Selectores

; Modificadores
(define addSymbol (lambda (C x)
                  (cons C x)))